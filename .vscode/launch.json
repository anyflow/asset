    "version": "0.2.0",
    "configurations": [{
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "NodeJS: Current File",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "env": {},
            "smartStep": true,
            "showAsyncStacks": true,
            "sourceMaps": true
        }
    ]
}